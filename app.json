      },
      "package": "com.amir398.frienddly_app_frontend"
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "plugins": ["expo-secure-store"],
    "extra": {
      "eas": {
        "projectId": "7fac6c77-17a2-45c3-bf1f-f09337376dd7"
